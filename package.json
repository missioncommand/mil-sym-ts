{
  "name": "@armyc2.c5isr.renderer/mil-sym-ts",
  "version": "2.1.1",
  "description": "MIL-STD-2525 D/E symbol rendering TypeScript library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "resolveJsonModule": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rd /S /Q dist",
    "build": "tsc --build tsconfig.json",
    "docs": "npx typedoc",
    "bundleES": "node buildES.js",
    "bundleESR": "node buildESR.js",
    "bundleWP": "webpack --config webpack.config.js",
    "bundleWPR": "webpack --config webpackr.config.js",
    "bundleDTS": "npx dts-bundle-generator -o dist/C5Ren.d.ts index.ts",
    "pack": "copy package.pack.json dist\\package.json && copy README.md dist\\README.md && copy LICENSE.txt dist\\LICENSE.txt && cd dist && del index.* && del tsconfig.tsbuildinfo && rd /s /q src && npm pack"
  },
  "author": "C5ISR ESI",
  "license": "GPL-3.0",
  "keywords": [
    "military",
    "symbology",
    "MIL-STD-2525",
    "rendering",
    "visualization"
  ],
  "devDependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "0.21.5",
    "jszip": "^3.10.1",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.3",
    "typedoc": "^0.26.3",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  }
}
